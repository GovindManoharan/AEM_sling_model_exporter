<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:crx="http://www.day.com/crx/1.0" xmlns:rep="internal"
    jcr:primaryType="rep:ACL">
    <allow
        jcr:primaryType="rep:GrantACE"
        rep:principalName="dynamicmedia-config-service"
        rep:privileges="{Name}[jcr:modifyProperties,jcr:read]"/>
    <allow1
        jcr:primaryType="rep:GrantACE"
        rep:principalName="dynamic-media-replication"
        rep:privileges="{Name}[jcr:read]">
        <rep:restrictions
            jcr:primaryType="rep:Restrictions"
            rep:glob="/dynamicmediaconfig"/>
    </allow1>
    <allow2
        jcr:primaryType="rep:GrantACE"
        rep:principalName="dynamic-media-replication"
        rep:privileges="{Name}[jcr:read,crx:replicate]">
        <rep:restrictions
            jcr:primaryType="rep:Restrictions"
            rep:glob="/imageserver/macros/*"/>
    </allow2>
    <allow3
        jcr:primaryType="rep:GrantACE"
        rep:principalName="dynamic-media-replication"
        rep:privileges="{Name}[jcr:read,crx:replicate]">
        <rep:restrictions
            jcr:primaryType="rep:Restrictions"
            rep:glob="/presets/*"/>
    </allow3>
    <allow4
        jcr:primaryType="rep:GrantACE"
        rep:principalName="dynamic-media-replication"
        rep:privileges="{Name}[jcr:read,crx:replicate]">
        <rep:restrictions
            jcr:primaryType="rep:Restrictions"
            rep:glob="/tenants/*/presets/*"/>
    </allow4>
    <allow5
        jcr:primaryType="rep:GrantACE"
        rep:principalName="dynamic-media-replication"
        rep:privileges="{Name}[jcr:read,crx:replicate]">
        <rep:restrictions
            jcr:primaryType="rep:Restrictions"
            rep:glob="/tenants/*/imageserver/configuration/jcr:content/settings"/>
    </allow5>
    <allow6
        jcr:primaryType="rep:GrantACE"
        rep:principalName="dynamic-media-s7sync"
        rep:privileges="{Name}[jcr:all]"/>
</jcr:root>
