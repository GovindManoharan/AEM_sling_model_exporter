<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:rep="internal"
    jcr:primaryType="rep:ACL">
    <allow
        jcr:primaryType="rep:GrantACE"
        rep:principalName="workflow-users"
        rep:privileges="{Name}[jcr:versionManagement,jcr:read,rep:write,jcr:lockManagement]"/>
    <allow1
        jcr:primaryType="rep:GrantACE"
        rep:principalName="workflow-service"
        rep:privileges="{Name}[jcr:all]"/>
    <allow2
        jcr:primaryType="rep:GrantACE"
        rep:principalName="translation-job-service"
        rep:privileges="{Name}[jcr:versionManagement,jcr:read,rep:write,jcr:lockManagement]"/>
</jcr:root>
