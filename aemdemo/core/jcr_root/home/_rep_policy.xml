<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:rep="internal"
    jcr:primaryType="rep:ACL">
    <deny
        jcr:primaryType="rep:DenyACE"
        rep:principalName="everyone"
        rep:privileges="{Name}[jcr:read]"/>
    <allow1
        jcr:primaryType="rep:GrantACE"
        rep:principalName="user-administrators"
        rep:privileges="{Name}[jcr:all]"/>
    <allow2
        jcr:primaryType="rep:GrantACE"
        rep:principalName="activity-service"
        rep:privileges="{Name}[jcr:addChildNodes,rep:readNodes,jcr:nodeTypeManagement]"/>
    <allow3
        jcr:primaryType="rep:GrantACE"
        rep:principalName="activity-service"
        rep:privileges="{Name}[rep:readProperties]">
        <rep:restrictions
            jcr:primaryType="rep:Restrictions"
            rep:itemNames="{Name}[jcr:primaryType,jcr:uuid]"/>
    </allow3>
    <allow4
        jcr:primaryType="rep:GrantACE"
        rep:principalName="activity-service"
        rep:privileges="{Name}[rep:addProperties]">
        <rep:restrictions
            jcr:primaryType="rep:Restrictions"
            rep:glob="*/activities/sling:resourceType"/>
    </allow4>
    <allow5
        jcr:primaryType="rep:GrantACE"
        rep:principalName="activity-service"
        rep:privileges="{Name}[rep:addProperties]">
        <rep:restrictions
            jcr:primaryType="rep:Restrictions"
            rep:glob="*/activities/*/sling:resourceType"/>
    </allow5>
    <allow6
        jcr:primaryType="rep:GrantACE"
        rep:principalName="security-userproperties-service"
        rep:privileges="{Name}[jcr:modifyAccessControl,jcr:read,jcr:readAccessControl]"/>
    <allow7
        jcr:primaryType="rep:GrantACE"
        rep:principalName="communities-user-admin"
        rep:privileges="{Name}[jcr:all]">
        <rep:restrictions
            jcr:primaryType="rep:Restrictions"
            rep:glob="*/activities/*"/>
    </allow7>
    <allow8
        jcr:primaryType="rep:GrantACE"
        rep:principalName="community-site-administrators"
        rep:privileges="{Name}[jcr:read]"/>
    <allow9
        jcr:primaryType="rep:GrantACE"
        rep:principalName="community-group-administrators"
        rep:privileges="{Name}[jcr:read]"/>
    <allow10
        jcr:primaryType="rep:GrantACE"
        rep:principalName="msm-service"
        rep:privileges="{Name}[jcr:read]"/>
    <allow11
        jcr:primaryType="rep:GrantACE"
        rep:principalName="everyone"
        rep:privileges="{Name}[jcr:read]">
        <rep:restrictions
            jcr:primaryType="rep:Restrictions"
            rep:glob="*/profiles/public"/>
    </allow11>
    <allow12
        jcr:primaryType="rep:GrantACE"
        rep:principalName="everyone"
        rep:privileges="{Name}[jcr:read]">
        <rep:restrictions
            jcr:primaryType="rep:Restrictions"
            rep:glob="*/profiles/public/*"/>
    </allow12>
</jcr:root>
