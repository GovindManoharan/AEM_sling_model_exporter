<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:rep="internal"
    jcr:primaryType="rep:ACL">
    <allow
        jcr:primaryType="rep:GrantACE"
        rep:principalName="user-reader-service"
        rep:privileges="{Name}[jcr:read]"/>
    <allow1
        jcr:primaryType="rep:GrantACE"
        rep:principalName="user-administration-service"
        rep:privileges="{Name}[jcr:all]"/>
    <allow2
        jcr:primaryType="rep:GrantACE"
        rep:principalName="compat-codeupgrade-service"
        rep:privileges="{Name}[jcr:modifyProperties,jcr:read]"/>
    <allow3
        jcr:primaryType="rep:GrantACE"
        rep:principalName="dam-reports-manager-service"
        rep:privileges="{Name}[jcr:read]">
        <rep:restrictions
            jcr:primaryType="rep:Restrictions"
            rep:glob="*/*/activities/dam*"/>
    </allow3>
    <allow4
        jcr:primaryType="rep:GrantACE"
        rep:principalName="dam-activitywriter-service"
        rep:privileges="{Name}[jcr:read,rep:write]">
        <rep:restrictions
            jcr:primaryType="rep:Restrictions"
            rep:glob="*/*/activities/dam*"/>
    </allow4>
    <allow5
        jcr:primaryType="rep:GrantACE"
        rep:principalName="notification-service"
        rep:privileges="{Name}[jcr:read,rep:write]">
        <rep:restrictions
            jcr:primaryType="rep:Restrictions"
            rep:glob="*/wcm/notification*"/>
    </allow5>
    <allow6
        jcr:primaryType="rep:GrantACE"
        rep:principalName="contentsync-service"
        rep:privileges="{Name}[rep:readNodes]">
        <rep:restrictions
            jcr:primaryType="rep:Restrictions"
            rep:ntNames="{Name}[rep:User]"/>
    </allow6>
    <allow7
        jcr:primaryType="rep:GrantACE"
        rep:principalName="contentsync-service"
        rep:privileges="{Name}[rep:readProperties]">
        <rep:restrictions
            jcr:primaryType="rep:Restrictions"
            rep:itemNames="{Name}[rep:authorizableId,rep:principalName,jcr:uuid,jcr:primaryType]"/>
    </allow7>
    <allow8
        jcr:primaryType="rep:GrantACE"
        rep:principalName="translation-job-service"
        rep:privileges="{Name}[jcr:read]"/>
    <allow9
        jcr:primaryType="rep:GrantACE"
        rep:principalName="activitypurgesrv"
        rep:privileges="{Name}[jcr:removeChildNodes,jcr:removeNode,rep:removeProperties,jcr:read]"/>
    <allow10
        jcr:primaryType="rep:GrantACE"
        rep:principalName="fd-service"
        rep:privileges="{Name}[jcr:read]"/>
    <allow11
        jcr:primaryType="rep:GrantACE"
        rep:principalName="social-enablement-replication-user"
        rep:privileges="{Name}[jcr:read]"/>
</jcr:root>
